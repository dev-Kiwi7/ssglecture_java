package table;

import javax.swing.JFrame;
import javax.swing.JTable;

public class TableTest extends JFrame{
	JTable table;
	String[][] data= {
			{"아우디","벤츠","BMW","K9"},
			{"아우디","벤츠","BMW","K9"},
			{"아우디","벤츠","BMW","K9"},
			{"아우디","벤츠","BMW","K9"}
	};
	public TableTest() {
		table = new JTable(3 ,4) ;
		add(table);
		
		setSize(400,300);
		setVisible(true);
		setDefaultCloseOperation(EXIT_ON_CLOSE);
	}
	public static void main(String[] 	args) {
		new TableTest();

	}

}
